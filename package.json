    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.0",
    "@types/react": "^16.14.4",
    "@types/react-dom": "^16.9.11",
    "node-sass": "^4.14.1",
